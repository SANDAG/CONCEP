    <!-- Queries -->

    <!-- ControlGQ Queries-->
    <add key ="selectControlGQ1" value ="Select city, gq_civ_college,gq_civ_other from {0} where estimates_year = {1}"/>
    <add key ="selectControlGQ2" value="Select x.jurisdiction_2010 as city,lckey,lu,g.mgra,gq,gq_civ,gq_mil,gq_civ_college,gq_civ_other from {0} g, {1} x
                                    where x.mgra_13 = g.mgra and estimates_year = {2} order by x.jurisdiction_2010,gq desc"/>
    <!-- ControlVitals Queries-->

    <add key ="deleteControlVitals1" value="DELETE FROM {0} WHERE {1} = {2}"/>
    <add key ="insertControlVitals1" value ="insert into {0} select {3} ,x.ct10,ethnicity,sex,sum(births) from {1} x, {2} c where birth_year = 
                                          {3} and x.mgra = c.mgra group by x.ct10,ethnicity,sex"/>

    <add key ="selectControlVitals1" value ="SELECT b.ethnicity, b.sex, b.age, SUM(b.pop) AS sumPop, s.rate, r.roc FROM {0} s, {1} r, {2} b 
                                         where s.ethnicity = r.ethnicity AND s.ethnicity = b.ethnicity and s.sex = r.sex AND s.sex = b.sex and 
                                         s.age = r.age and s.age = b.age AND b.estimates_year = {3} GROUP BY b.ethnicity, b.sex, b.age, s.rate, r.roc ORDER BY b.ethnicity, b.sex, b.age"/>

    <add key ="selectControlVitals2" value="select distinct zip from {0} order by zip"/>
    <add key ="selectControlVitals3" value="select zip, births, hm, nhwm, nhbm, nhim, nham, nhhm, nhom,nh2m, tm, hf, nhwf, nhbf, nhif, nhaf, nhhf, nhof, nh2f, tf from {0}
                                           where birth_year = {1} order by zip"/>

    <add key ="selectControlVitals4" value="select distinct mgra,zip from {0} order by zip, mgra"/>
    <add key ="selectControlVitals5" value ="SELECT mgra, ethnicity, popf_18to19+popf_20to24+popf_25to29+popf_30to34+popf_35to39 FROM {0} WHERE estimates_year = {1} and ethnicity &gt; 0"/>
    <add key ="selectControlVitals6" value ="SELECT ct10, ethnicity, sex, age, pop FROM {0} WHERE estimates_year = {1}"/>
    <add key ="selectControlVitals7" value ="SELECT ethnicity, sex, age, SUM(pop) from {0} where estimates_year = {1} and pop &gt; 0 GROUP BY ethnicity, sex, age ORDER BY ethnicity, sex, age"/>
    <add key ="selectControlVitals8" value="SELECT eth, dcm, dcf FROM {0} WHERE birth_death_year = {1}"/>

    <!-- IncomeCalib Queries-->
    <add key="deleteIncomeCalib1" value ="delete from {0} where geo_type = {1}"/>

    <add key ="selectIncomeCalib1" value ="select * from {0} where geo_type = {1}"/>

    <!-- POPEST Queries-->
    <add key ="selectPOPEST1" value ="Select city,hs,hs_sf,hs_sfmu,hs_mf,hs_mh,hh,hh_sf,hh_sfmu,hh_mf,hh_mh,hhp,gq,gq_civ,gq_mil,gq_civ_college,
                                     gq_civ_other,vac_sf,vac_sfmu,vac_mf,vac_mh,hhs from {0} where estimates_year = {1} order by city"/>

    <add key ="selectPOPEST2" value="select t.mgra, x.city, SUM(hs_sf), SUM(hs_sfmu), SUM(hs_mf), SUM(hs_mh) from {0} t, {1} x 
                                     where t.estimates_year = {2} AND t.mgra = x.mgra GROUP BY t.mgra, x.city"/>

    <add key="selectPOPEST3" value ="select p.mgra, p.cityct10, p.city, p.ct10, p.hs, p.hs_sf, p.hs_sfmu, p.hs_mf, p.hs_mh, p.gq, p.gq_civ, 
                                    p.gq_mil, p.gq_civ_college, p.gq_civ_other,p.hhp, p.hh, p.hh_sf, p.hh_sfmu, p.hh_mf, p.hh_mh, po.hs,  
                                    p.vac_sf, p.vac_sfmu, p.vac_mf, p.vac_mh, p.vac, p.hhs from {0} p, {0} po where p.estimates_year = {1} AND po.estimates_year = {2}
                                    AND p.mgra = po.mgra"/>

    <add key ="insertPOPEST1" value="insert into {0} select {1} , mgra, cityct10, city, ct10, pop, hhp, gq, gq_civ, gq_mil, gq_civ_college,gq_civ_other,
                                  hs, hs_sf, hs_sfmu, hs_mf, hs_mh, hh, hh_sf, hh_sfmu, hh_mf, hh_mh, vac, vac_sf, vac_sfmu, vac_mf, vac_mh, hhs from {0} 
                                  WHERE estimates_year = {2}"/>

    <add key ="updatePOPEST1" value="UPDATE {0} SET hs = {1} ,hs_sf = {2} ,hs_sfmu = {3} ,hs_mf = {4}, hs_mh = {5} WHERE estimates_year = {6} AND mgra = {7}"/>

    <add key="updatePOPEST2" value ="update {0} set pop = p2.pop, hhp = p2.hhp, gq = p2.gq, gq_civ = p2.gq_civ,
                                     hs = p2.hs, hs_sf = p2.hs_sf, hs_sfmu = p2.hs_sfmu, hs_mf = p2.hs_mf, hs_mh = p2.hs_mh,
                                     hh = p2.hh, hh_sf = p2.hh_sf, hh_sfmu = p2.hh_sfmu, hh_mf = p2.hh_mf, hh_mh = p2.hh_mh, 
                                     gq_civ_college = p2.gq_civ_college, gq_civ_other = p2.gq_civ_other  from {0} p, {1} p2 
                                     where p.estimates_year = {2} and p.mgra = p2.mgra"/>

    <add key ="updatePOPEST3" value="update {0} set {1} where estimates_year = {2}"/>
    <add key ="updatePOPEST4" value ="update {0} set {1} where estimates_year = {2} and {3}"/>

    <add key ="updatePOPEST5" value ="WITH g as (select mgra, sum(gq_civ) as gq_civ, sum(gq_mil) as gq_mil, sum(gq_civ_college) as 
                                gq_civ_college, sum(gq_civ_other) as gq_civ_other FROM {1} where estimates_year = {2} group by mgra) 
                                UPDATE {0} SET gq_civ = g.gq_civ, gq_civ_college = g.gq_civ_college, gq_civ_other = g.gq_civ_other, 
                                gq_mil = g.gq_mil FROM {0} p, g WHERE p.mgra = g.mgra AND p.estimates_year = {2}"/>

    <add key ="updatePOPEST6" value ="update {0} set gq = gq_mil + gq_civ where estimates_year = {1}"/>
    <add key ="updatePOPEST7" value="update {0} set hhs = o.hhs_override from {0} p, {1} o where p.mgra = o.mgra and p.estimates_year = {2}"/>
    <add key ="updatePOPEST8" value="update {0} set vac_sf = o.vac_sf_override from {0} p, {1} o where o.vac_sf_override &gt; 0 and p.mgra = o.mgra and p.estimates_year = {2}"/>
    <add key ="updatePOPEST9" value="update {0} set vac_sfmu = o.vac_sfmu_override from {0} p, {1} o where o.vac_sfmu_override &gt; 0 and p.mgra = o.mgra and p.estimates_year = {2}"/>
    <add key ="updatePOPEST10" value="update {0} set vac_mf = o.vac_mf_override from {0} p, {1} o where o.vac_mf_override &gt; 0 and p.mgra = o.mgra and p.estimates_year = {2}"/>
    <add key ="updatePOPEST11" value ="update {0} set gq_civ = 0, gq_mil = 0 , gq_civ_college = 0,gq_civ_other = 0 WHERE estimates_year = {1}"/>

    <!-- ESTINC Queries -->

    <add key ="selectESTINC1" value="select sum(hh) from {0} where estimates_year = {1}"/>
    <add key ="selectESTINC2" value ="select x.ct10,sum(hh) from {0} p, {1} x where p.estimates_year = {2} and x.mgra = p.mgra and x.sra = {3} group by x.ct10"/>
    <add key ="selectESTINC3" value ="select x.sra,sum(hh) from {0} p, {1} x where p.estimates_year = {2} and x.mgra = p.mgra group by x.sra"/>
    <add key ="selectESTINC4" value ="select * from {0} where estimates_year = {1} and geo_type = {2}"/>
    <add key ="selectESTINC5" value ="select * from {0} {1} "/>
    <add key ="selectESTINC6" value="select geo_type, id, i1, i2, i3, i4, i5, i6, i7, i8, i9, i10 from {0} where estimates_year = {1} {2}"/>
    <add key ="selectESTINC7" value ="select id, i1, i2, i3, i4, i5, i6, i7, i8, i9, i10, hh, median from {0} where estimates_year = {1} and geo_type = 2"/>
    <add key ="selectESTINC8" value ="SELECT mgra, i1, i2, i3, i4, i5, i6, i7, i8, i9, i10 FROM {0} WHERE estimates_year = {1} ORDER BY mgra"/>
    <add key ="selectESTINC9" value ="select id, i1, i2, i3, i4, i5, i6, i7, i8, i9, i10 from {0} where estimates_year = {1} and geo_type = 11"/>
    <add key ="selectESTINC10" value="select geo_id,parameter_type,parameter from {0} where estimates_year = {1} {2}"/>
    <add key ="selectESTINC11" value="select parameter_type, parameter from {0} where estimates_year = {1} and geo_type = 4 and geo_id = 999"/>
    <add key ="selectESTINC12" value="select geo_id,parameter_type,parameter from {0} where estimates_year = {1} and geo_type = 2"/>
    <add key ="selectESTINC13" value ="select distinct ct10 from {0} where estimates_year = {1} order by ct10"/>
    <add key ="selectESTINC14" value ="select x.ct10, p.mgra, p.hh from {0} p, {1} x where p.estimates_year = {2} and x.mgra = p.mgra order by p.mgra"/>

    <!-- PASEE Queries -->
    <add key ="selectPASEE1" value ="select distinct sra,ct10 from {0} order by ct10"/>
    <add key ="selectPASEE2" value ="SELECT COUNT(*) FROM {0}"/>
    <add key ="selectPASEE3" value ="select sra,ct10,ethnicity,sex,age,pop from {0} where estimates_year = {1} and ethnicity &gt; 0 and pop &gt; 0"/>
    <add key ="selectPASEE4" value ="select x.sra,x.ct10,sum(pop),sum(hhp),sum(gq_mil) from {0} p, {1} x where estimates_year = {2} and p.mgra = x.mgra group by x.sra,x.ct10"/>
    <add key ="selectPASEE5" value ="select ct10,ethnicity,sex,age5,pop from {0} a, {1} a5 where estimates_year = {2} AND a.age = a5.age"/>
    <add key ="selectPASEE6" value ="select ct10,mgra,pop from {0} WHERE estimates_year = {1}"/>
    <add key ="selectPASEE7" value ="select ct10,ethnicity,sex,sum(births) from {0} where birth_year = {1} group by ct10,ethnicity,sex"/>
    <add key ="selectPASEE8" value ="select ct10,ethnicity,sex,age,deaths from {0} where death_year = {1}"/>
    <add key ="updatePASEE1" value ="update {0} set pop_0to4 = popm_0to4 + popf_0to4, pop_5to9 = popm_5to9 + popf_5to9,pop_10to14 = popm_10to14 + popf_10to14,
                                     pop_15to17 = popm_15to17 + popf_15to17,pop_18to19 = popm_18to19 + popf_18to19,pop_20to24 = popm_20to24 + popf_20to24,
                                     pop_25to29 = popm_25to29 + popf_25to29,pop_30to34 = popm_30to34 + popf_30to34,pop_35to39 = popm_35to39 + popf_35to39,
                                     pop_40to44 = popm_40to44 + popf_40to44,pop_45to49 = popm_45to49 + popf_45to49,pop_50to54 = popm_50to54 + popf_50to54,
                                     pop_55to59 = popm_55to59 + popf_55to59,pop_60to61 = popm_60to61 + popf_60to61,pop_62to64 = popm_62to64 + popf_62to64,
                                     pop_65to69 = popm_65to69 + popf_65to69,pop_70to74 = popm_70to74 + popf_70to74,pop_75to79 = popm_75to79 + popf_75to79,
                                     pop_80to84 = popm_80to84 + popf_80to84,pop_85plus = popm_85plus + popf_85plus where estimates_year = {1}"/>

    <add key ="updatePASEE2" value ="update {0} set popm = (popm_0to4 + popm_5to9 + popm_10to14 + popm_15to17 + popm_18to19 + popm_20to24 + popm_25to29 + 
                                     popm_30to34 + popm_35to39 + popm_40to44 + popm_45to49 + popm_50to54 + popm_55to59 + popm_60to61 + popm_62to64 + 
                                     popm_65to69 + popm_70to74 + popm_75to79 + popm_80to84 + popm_85plus), popf = (popf_0to4 + popf_5to9 + popf_10to14 + popf_15to17 + 
                                     popf_18to19 + popf_20to24 + popf_25to29 + popf_30to34 + popf_35to39 + popf_40to44 + popf_45to49 + popf_50to54 + popf_55to59 + 
                                     popf_60to61 + popf_62to64 + popf_65to69 + popf_70to74 + popf_75to79 + popf_80to84 + popf_85plus) where estimates_year = {1}"/>

    <add key ="updatePASEE3" value ="update {0} set pop = popm + popf where estimates_year = {1}"/>

    <!-- Census 2010 Queries -->
    <add key ="selectCensus1" value ="select x.mgra,x.{2},sum(hs) from {0} x, {1} l where l.mgra = x.mgra and x.{2} &gt; 0 group by x.mgra,x.{2} order by x.mgra"/>
    <add key ="selectCensus2" value ="select {1},gq,coll,mil,other from {0} order by {1}"/>
    <add key ="selectCensus3" value ="select distinct ct10,block,ct10block from {0} order by ct10block"/>
    <add key ="selectCensus4" value ="select distinct mgra,ct10 from {0} order by mgra"/>
    <add key ="selectCensus5" value="select distinct mgra,{1} from {0} where {1} &gt; 0 order by mgra"/>
    <add key ="selectCensus6" value ="select * from {0} where {1} &gt; 0 and (pop + hs &gt; 0 or hs &gt; 0)"/>
    <add key ="selectCensus7" value ="select ct10block,{1},hhwc,hhwoc from {0}"/>
    <add key ="selectCensus8" value="select {2} ,lckey,c.lu,c.mgra,c.acres,c.gq,civcollege,mil,other from {0} c, {1} p where c.lu = p.lu order by {2}"/>
    <add key ="selectCensus9" value ="select ct10block,mgra from {0} order by ct10block"/>

